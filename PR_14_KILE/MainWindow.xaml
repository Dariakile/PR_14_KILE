<Window x:Class="PR_14_KILE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR_14_KILE"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" Icon="/Image/512.ico"
        WindowStartupLocation="CenterScreen">

<Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="83*"/>
            <RowDefinition Height="400*"/>
            <RowDefinition Height="54*"/>
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal" Background="CornflowerBlue">
            <Image Source="/Image/512.ico" />

            <TextBlock Text="Су-Виды" TextAlignment="Center" Style="{StaticResource TextW}"/>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Background="CornflowerBlue" Grid.Row="2">

        </StackPanel>

        <Frame x:Name="myFrame" NavigationUIVisibility="Hidden" Grid.Row="1" />
    </Grid>
</Window>
